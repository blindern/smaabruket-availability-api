{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "major": {
    "automerge": false
  },
  "automerge": true,
  "automergeType": "branch",
  "pin": {
    "automerge": true
  },
  "packageRules": [
    {
      "matchPackageNames": ["elysia", "@elysiajs/cors"],
      "groupName": "elysia"
    }
  ]
}
